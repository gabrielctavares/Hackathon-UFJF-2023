struct Endereco 
{
	string  logradouro;
	string 	complemento;
	string bairro;
	string cidade;
	string	estado;
	string	cep;
};
Pessoa{
	attribute string nome;
	attribute struct Endereco endereco;
}

Class PessoaFisica extends Pessoa
{
	attribute long cpf;
}

Class PessoaJuridica extends Pessoa
{
  attribute long cnpj;
}